{
  "name": "elasticsearch-sink-connector",
  "config": {
    "connector.class": "com.vimond.elasticsearch_kafka_connector.sink.ElasticsearchSinkConnector",
    "tasks.max": "4",
    "topics": "vimond-insights-topic",
    "geo.path": "/usr/local/share/GeoIP",
    "es.hostsAndPort": "elasticsearch.service.consul:9300",
    "es.index": "vimond-insights",
    "es.types": "user-asset_playback-event,order-domain-event,user-domain-event",
    "es.client.type": "transport",
    "es.cluster.name": "packetbeat",
    "es.input.json": "true",
    "rest-api.address" : "vimond-rest-api.ha.dev.vops.io",
    "discovery.zen.ping.multicast.enabled": "false",
    "discovery.zen.ping.unicast.hosts": "elasticsearch.service.consul:9200",
    "in-memory-map.cache.time": 2
  }
}